	<parameter name="id_classe" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT 
id_classe, 
nom_prenom,
(SELECT COUNT( id)  FROM eleves WHERE eleves.id_classe = $P{id_classe}  ) AS effectif,
classe,
annee
FROM eleves 
JOIN classes ON 
eleves.id_classe = classes.id
JOIN annees ON 
classes.id_annee = annees.id
